# Node.js / React
frontend/node_modules/        # Node modules
frontend/dist/                # Build output (e.g., for production)
frontend/build/               # Build output
frontend/.cache/              # Cache folder
frontend/.next/               # Next.js build folder
frontend/.vercel/             # Vercel deployment folder
frontend/npm-debug.log*       # NPM debug logs
frontend/yarn-debug.log*      # Yarn debug logs
frontend/yarn-error.log*      # Yarn error logs

# Python / FastAPI
backend/__pycache__/          # Python bytecode cache
backend/*.py[cod]             # Python bytecode files
backend/venv/                 # Virtual environment
backend/.venv/                # Virtual environment
backend/.vscode/              # VSCode settings folder
backend/.idea/                # JetBrains IDE settings folder
backend/*.iml                 # JetBrains project files
backend/.classpath            # Eclipse project settings
backend/.project              # Eclipse project settings
backend/.settings/            # Eclipse settings folder

# Environment files
.env                           # Local environment variables
.env.local                     # Local environment variables for development
.env.*.local                   # Local environment variables with specific suffixes

# Operating system files
.DS_Store                      # macOS system file
Thumbs.db                      # Windows system file

# Temporary and log files
*.log                          # Log files
*.tmp                          # Temporary files
*.swp                          # Swap files

# IDE and editor settings
.vscode/                       # Visual Studio Code settings
.idea/                         # JetBrains IDE settings
